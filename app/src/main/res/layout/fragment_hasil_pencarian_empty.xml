<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.hasilpencarianempty.HasilPencarianEmptyFragment">

    <ImageView
        android:layout_width="276dp"
        android:layout_height="222dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="200dp"
        android:src="@drawable/ilustrasi_search_" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_marginTop="60dp">

        <ImageView
            android:id="@+id/btnFilter"
            android:layout_marginLeft="15dp"
            android:layout_width="102dp"
            android:layout_height="47dp"
            android:src="@drawable/btnpth" />

        <ImageView
            android:id="@+id/btnTermurah"
            android:layout_marginRight="15dp"
            android:layout_width="121dp"
            android:layout_height="47dp"
            android:layout_alignParentRight="true"
            android:src="@drawable/btnungu" />

    </RelativeLayout>

    <ImageView
        android:layout_width="287dp"
        android:layout_height="48dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="450dp"
        android:src="@drawable/empty"/>


    <Button
        android:id="@+id/btn_ubahPencarian"
        style="@style/ButtonPrimary"
        android:backgroundTint ="@color/darkblue_04"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/ubah_pencarian"
        android:layout_marginTop="630dp"
        android:layout_marginBottom="10dp"
        android:layout_marginHorizontal="15dp" />






</FrameLayout>